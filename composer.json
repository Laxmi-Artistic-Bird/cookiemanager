{
    "name": "artisticbird/cookies",
    "description": "This will used to manage web cookies",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Artisticbird\\Cookies\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Artisticbird-Tech-Pvt-Ltd",
            "email": "nfo@artisticbird.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "extra": {
        "laravel":{
            "providers":[
                "Artisticbird\\Cookies\\CookiesServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php ../../artisan migrate --path=Database\\migrations",
            "@php ../../artisan db:seed --class=Seeds\\CookieCategorySeeder"
        ],
        "post-update-cmd": [
            "@php ../../artisan migrate --path=Database\\migrations",
            "@php ../../artisan db:seed --class=Database\\Seeds\\CookieCategorySeeder"
        ]
    }
}
